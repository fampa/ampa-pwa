{
  "extends": "@quasar/app/tsconfig-preset",
  "compilerOptions": {
    "module": "commonjs",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "outDir": "lib",
    "sourceMap": true,
    "strict": true,
    "target": "es2017",
    "noImplicitAny": false,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
  },
  "include": [
    "src",
    "functions"
  ],
  "exclude": [
    "node_modules",
    "functions/node_modules"
  ]
}

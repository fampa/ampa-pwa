{
  "name": "ampa-pwa",
  "version": "0.0.1",
  "description": "PWA de gesti√≥ del AMPA",
  "productName": "AMPA PWA",
  "author": "Josep V. Monjo <jvmonjo@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "quasar dev",
    "dev:functions": "cd functions && yarn serve",
    "generate:icons": "icongenie generate -i src/assets/icon.png",
    "prebuild": "browserify -t localenvify src-pwa/firebase-messaging-sw.js > public/firebase-messaging-sw.js",
    "build": "yarn && npm run build:functions && quasar build -m pwa",
    "build:functions": "cd functions && yarn install --ignore-engines && cd ..",
    "deploy": "npm run build && firebase deploy --only hosting",
    "deploy:functions": "npm run build:functions && firebase deploy --only functions",
    "lint": "eslint --ext .js,.ts,.vue ./ --fix",
    "test": "echo \"No test specified\" && exit 0"
  },
